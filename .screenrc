shell -$SHELL

scrollback 10000
utf8 on
defutf8 on
defencoding utf8
encoding utf-8 utf-8

backtick 1 0 0 $HOME/pyenv/python3/slack/main.py
caption always "%1`"

hardstatus alwayslastline '%{= .y}%H %L=%{= .b}%-w%46>%{= .r}%n %t*%{= .b}%+w%-16=%{= .y}[%l]'
termcapinfo xterm 'is=\E[r\E[m\E[2J\E[H\E[?7h\E[?1;4;6l'
escape ^\\
